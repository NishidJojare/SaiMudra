    .x {
        margin-top: 87px;
    }
</style>


<script src="https://cdnjs.cloudflare.com/ajax/libs/datatables/1.10.21/js/jquery.dataTables.min.js" defer="defer"></script>
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/datatables/1.10.21/css/dataTables.bootstrap.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/datatables-buttons/2.3.6/js/dataTables.buttons.min.js" defer="defer"></script>

<main class="main" id="main">
    <div class="x">

        <table id="tblKattarVadak" class="table table-striped table-bordered" border="1">
            @*<thead>
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                        <th>Instagram</th>
                        <th>Facebook</th>
                        <th>IsActive</th>
                        <th>CreateDate</th>
                        <th>Photo</th>
                    </tr>
                </thead>
                <tbody></tbody>*@
        </table>
    </div>
</main>
<div id="WriteToUsModal" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-bs-dismiss="modal">&times;</button>
                <h1>Write To Us &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp</h1>
            </div>

            <div class="modal-body">
                <main id="main" class="main">
                    <input type="hidden" id="hdnId" />
                    <div class="card">
                        <div class="card-body">
                            <h2 class="card-title" style="text-align:center">Register Yourself</h2>

                            <form class="row g-3">
                                <div class="col-sm-12">
                                    <label class="form-label">Name</label>
                                    <input type="text" class="form-control" placeholder="Your Name" id="txtName">
                                </div>
                                <div class="col-sm-6">
                                    <label class="form-label">Facebook Link</label>
                                    <input type="text" class="form-control" placeholder="Facebook Link" id="txtFacebook">
                                </div>
                                <div class="col-sm-6">
                                    <label class="form-label">Instagram Link</label>
                                    <input type="text" class="form-control" placeholder="Instagram Link" id="txtInstragram">
                                </div>
                                <div class="col-sm-6">
                                    <label class="form-label">Upload Photo</label>
                                    <input class="form-control" type="file" id="file1">
                                </div>

                                <div class="col-sm-6" style="margin-top:50px;">

                                    <input class="form-check-input" type="checkbox" id="txtIsActive">
                                    <label class="form-check-label">
                                        Agree all Terms and Conditions
                                    </label>

                                </div>
                                <div class="text-center">
                                    <input type="button" value="Submit" class="btn btn-primary" onclick="SaveKatterVadak()" />
                                    &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;
                                    <button type="reset" class="btn btn-secondary">Reset</button>
                                </div>
                            </form><!-- End No Labels Form -->

                        </div>
                    </div>
                </main>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bs-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<div id="KattarModal" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-bs-dismiss="modal">&times;</button>
                <h1>Write To Us &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp</h1>
            </div>

            <div class="modal-body">
                <div class="container" id="KattarDetail">
                </div>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bs-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>